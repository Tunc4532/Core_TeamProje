@using EntityLayer.Concreate
@model List<UzmanEkip>

<div class="col-md-12 grid-margin stretch-card">
    <div class="card">
        <div class="card-body">
            <p class="card-title">Ekip Uzmanlık Alanlarımız</p>
            <div class="charts-data">
                <div class="mt-3">
                    @foreach(var item in Model)
                    {
                    <p class="mb-0">@item.Tittle</p>
                    <div class="d-flex justify-content-between align-items-center">
                        <div class="progress progress-md flex-grow-1 mr-4">
                            <div class="progress-bar bg-inf0" role="progressbar" style="width: @item.Value%" aria-valuenow="@item.Value" aria-valuemin="0" aria-valuemax="100"></div>
                        </div>
                        <p class="mb-0">@item.Value %</p>
                    </div>
                    }
                </div>
            </div>
        </div>
    </div>
</div>